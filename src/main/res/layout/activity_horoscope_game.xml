<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="abassawo.c4q.nyc.cheesyhoroscope.HoroscopeGame">




<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textAppearance="?android:attr/textAppearanceMedium"
    android:text="Daily Horoscope"
    android:id="@+id/titleA"
    android:layout_alignParentTop="true"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true" />

<GridView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/gridView2"
    android:choiceMode="singleChoice"
    android:numColumns="3"
    android:layout_alignRight="@+id/titleA"
    android:layout_alignEnd="@+id/titleA" />

<ImageButton
    android:layout_width="80dp"
    android:layout_height="100dp"
    android:id="@+id/ariesButton"
    android:src="@drawable/ariestat"
    android:layout_alignTop="@+id/gridView2"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true" />

<ImageButton
    android:layout_width="80dp"
    android:layout_height="100dp"
    android:id="@+id/taurusButton"
    android:src="@drawable/taurustat"
    android:layout_alignParentTop="true"
    android:layout_centerHorizontal="true" />

<ImageButton
    android:layout_width="80dp"
    android:layout_height="100dp"
    android:id="@+id/geminiButton"
    android:src="@drawable/geminitat"
    android:layout_above="@+id/cancerButton"
    android:layout_alignLeft="@+id/sagButton"
    android:layout_alignStart="@+id/sagButton" />

<ImageButton
    android:layout_width="80dp"
    android:layout_height="100dp"
    android:id="@+id/cancerButton"
    android:src="@drawable/cancertat"
    android:layout_below="@+id/ariesButton"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true" />

<ImageButton
    android:layout_width="80dp"
    android:layout_height="100dp"
    android:id="@+id/leoButton"
    android:src="@drawable/leotat"
    android:layout_alignTop="@+id/cancerButton"
    android:layout_centerHorizontal="true" />

<ImageButton
    android:layout_width="80dp"
    android:layout_height="100dp"
    android:id="@+id/virgoButton"
    android:src="@drawable/virgotat"
    android:layout_alignTop="@+id/leoButton"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true" />

<ImageButton
    android:layout_width="80dp"
    android:layout_height="100dp"
    android:id="@+id/libraButton"
    android:src="@drawable/libratat"
    android:layout_below="@+id/leoButton"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="73dp" />

<ImageButton
    android:layout_width="80dp"
    android:layout_height="100dp"
    android:id="@+id/scorpioButton"
    android:src="@drawable/scorpiotat"
    android:layout_alignTop="@+id/libraButton"
    android:layout_alignLeft="@+id/taurusButton"
    android:layout_alignStart="@+id/taurusButton" />

<ImageButton
    android:layout_width="80dp"
    android:layout_height="100dp"
    android:id="@+id/sagButton"
    android:src="@drawable/sagittariustat"
    android:layout_alignBottom="@+id/scorpioButton"
    android:layout_alignRight="@+id/gridView2"
    android:layout_alignEnd="@+id/gridView2" />

<ImageButton
    android:layout_width="80dp"
    android:layout_height="100dp"
    android:id="@+id/capricornButton"
    android:src="@drawable/capricorntat"
    android:layout_below="@+id/scorpioButton"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true" />

<ImageButton
    android:layout_width="80dp"
    android:layout_height="100dp"
    android:id="@+id/aquariusButton"
    android:src="@drawable/aquariustat"
    android:layout_below="@+id/scorpioButton"
    android:layout_alignLeft="@+id/scorpioButton"
    android:layout_alignStart="@+id/scorpioButton" />

<ImageButton
    android:layout_width="80dp"
    android:layout_height="100dp"
    android:id="@+id/piscesButton"
    android:src="@drawable/piscestat"
    android:layout_alignBottom="@+id/gridView2"
    android:layout_alignLeft="@+id/sagButton"
    android:layout_alignStart="@+id/sagButton" />

</RelativeLayout>
